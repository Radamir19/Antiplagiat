{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/files/upload",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [ { "Host": "filestorage", "Port": 8080 } ],
      "UpstreamPathTemplate": "/gateway/upload",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/analysis/check",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [ { "Host": "analysis", "Port": 8080 } ],
      "UpstreamPathTemplate": "/gateway/analyze",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/analysis/reports/{taskId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [ { "Host": "analysis", "Port": 8080 } ],
      "UpstreamPathTemplate": "/gateway/reports/{taskId}",
      "UpstreamHttpMethod": [ "GET" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5005"
  }
}